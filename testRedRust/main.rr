class A:
	i32 a;

	A():
		this.a = 1;

	A(i32 a):
		this.a = a;

	void Add():
		this.a = this.a + 1;

	void Add(i32 b):
		this.a = this.a + b;

class B:
	string b;

	B():
		this.b = "Allo";

	void Print():
		print("Hello World {");
		print(this.b);
		print("}\n");

class C(A):
	str id;
	B b;

	C(str id):
		base();
		this.id = id;
		this.b = new B();

	C(i32 a,str id):
		base(a);
		this.id = id;
		this.b = new B();

fn B createB():
	B b = new B();
	return b;

fn C createC(i32? a,str id):
	if a:
		return new C(a,id);
	return new C(id);

fn i32 main():
	C a = createC(null,"ID");
	print(a.id);
	print(":\n");
	print(a.a);
	print("\n");
	a.Add();
	print(a.a);
	print("\n");
	
	a = createC(5,"ID2");
	a = createC(6,"ID3");
	print(a.id);
	print(":\n");
	print(a.a);
	print("\n");
	a.Add(5);
	print(a.a);
	print("\n");

	B b = createB();
	b.Print();
	return 0;